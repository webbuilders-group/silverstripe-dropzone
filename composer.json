{
    "name": "webbuilders-group/silverstripe-dropzone",
    "description": "An HTML5 upload field for the CMS and frontend forms.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "upload", "uploader", "files", "forms", "cms"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ed Chipman",
            "homepage": "https://webbuildersgroup.com",
            "role": "Developer"
        },
        {
            "name": "Uncle Cheese",
            "email": "unclecheese@leftandmain.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4|^5"
    },
    "extra": {
        "installer-name": "dropzone",
        "expose": [
            "javascript",
            "css",
            "images"
        ]        
    },
    "autoload": {
        "psr-4": {
            "UncleCheese\\Dropzone\\": "src/"
        }
    },
    "replace": {
        "unclecheese/dropzone": "*"
    }
}